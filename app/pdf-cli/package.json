{
  "name": "@spike/pdf-cli",
  "version": "1.14.0",
  "description": "Spike cli app to find and process pdfs using the Spike API",
  "main": "src/app.js",
  "homepage": "https://spikedata.co.za",
  "author": "ilan@spikedata.co.za",
  "repository": {
    "type": "git",
    "url": "https://github.com/spikedata/pub"
  },
  "license": "MIT",
  "keywords": [
    "spike",
    "pdf",
    "south africa",
    "bank",
    "statements"
  ],
  "scripts": {
    "build": "run-s clean && run-p build:*",
    "build:cli": "rsync src/cli.js dist/",
    "build:app": "webpack --config ./src/webpack.config.js",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"{src,test}/**/*.{js,cjs,mjs,ts}\" --write",
    "fix:lint": "eslint \"{src,test}/**/*.{js,cjs,mjs,ts}\" --quiet --fix",
    "clean": "trash dist",
    "pre-publish": "run-s build test",
    "test": "./test/pdf-cli-test.sh"
  },
  "bin": {
    "spike-pdf-cli": "dist/cli.js",
    "spike-pdf-cli-update": "scripts/update.bat"
  },
  "files": [
    "dist/",
    "scripts/"
  ],
  "dependencies": {
    "@fast-csv/format": "^4.3.5",
    "@spike/api": "^1.0.0",
    "assert": "^2.0.0",
    "async": "3.1.0",
    "chalk": "2.4.2",
    "deep-extend": "0.6.0",
    "duration": "0.2.2",
    "json5": "2.1.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.3",
    "minimatch": "3.0.4",
    "path-browserify": "^1.0.1",
    "read": "1.0.7",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@vercel/ncc": "^0.28.6",
    "babel-loader": "^8.2.2",
    "webpack": "^5.38.1"
  }
}
